{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "checkJs": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "react-jsxdev",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ES2017",
    "types": ["vite/client"]
  }
}
